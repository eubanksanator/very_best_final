<%= simple_form_for(@favorite) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :notes %>

    <!-- Get rid of this dropdown -->
    <%= f.association :user %>
    <!--  -->

    <%= f.association :dish %>
    <%= f.association :venue %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
